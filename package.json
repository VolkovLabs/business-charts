{
  "author": "Volkov Labs",
  "dependencies": {
    "@emotion/css": "^11.11.0",
    "@grafana/data": "9.5.2",
    "@grafana/runtime": "9.5.2",
    "@grafana/ui": "9.5.2",
    "@types/echarts": "^4.9.17",
    "@types/googlemaps": "^3.43.3",
    "echarts": "^5.4.2",
    "echarts-extension-amap": "^1.11.0",
    "echarts-extension-gmap": "^1.6.0",
    "echarts-gl": "^2.0.9",
    "echarts-liquidfill": "^3.1.0",
    "echarts-stat": "^1.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.5.0"
  },
  "description": "Apache ECharts panel for Grafana",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@grafana/e2e": "^9.5.2",
    "@grafana/e2e-selectors": "^9.5.2",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.60",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.16",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.4",
    "eslint-plugin-react": "^7.32.2",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.62.1",
    "sass-loader": "13.2.2",
    "style-loader": "3.3.2",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0",
  "name": "volkovlabs-echarts-panel",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "levitate": "npx @grafana/levitate@latest is-compatible --path src/module.ts --target @grafana/data,@grafana/runtime,@grafana/ui",
    "lint": "eslint --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "docker-compose pull && docker-compose up",
    "stop": "docker-compose down",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --maxWorkers 4",
    "upgrade": "npm upgrade --save"
  },
  "version": "4.5.0"
}
