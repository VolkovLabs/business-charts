{
  "author": "Volkov Labs",
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "^10.0.0",
    "@grafana/runtime": "^10.0.3",
    "@grafana/ui": "^10.0.0",
    "@types/echarts": "^4.9.18",
    "@types/googlemaps": "^3.43.3",
    "echarts": "^5.4.3",
    "echarts-extension-amap": "^1.11.0",
    "echarts-extension-gmap": "^1.6.0",
    "echarts-gl": "^2.0.9",
    "echarts-liquidfill": "^3.1.0",
    "echarts-stat": "^1.2.0",
    "echarts-wordcloud": "^2.1.0",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "18.2.0",
    "tslib": "^2.6.1",
    "uuid": "^9.0.0"
  },
  "description": "Apache ECharts panel for Grafana",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@grafana/e2e": "^10.0.3",
    "@grafana/e2e-selectors": "^10.0.0",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.3.75",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.28",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.196",
    "@types/node": "^18.17.4",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/uuid": "^9.0.3",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@volkovlabs/jest-selectors": "^1.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint-plugin-deprecation": "^1.5.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.3.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "prettier": "^3.0.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.64.2",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0",
  "name": "volkovlabs-echarts-panel",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e": "npm exec cypress install && npm exec grafana-e2e run",
    "e2e:update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "levitate": "npx @grafana/levitate@latest is-compatible --path src/module.ts --target @grafana/data,@grafana/runtime,@grafana/ui",
    "lint": "eslint --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ignore-path ./.eslintignore --ext .js,.jsx,.ts,.tsx .",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "start": "docker-compose pull && docker-compose up",
    "stop": "docker-compose down",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --maxWorkers 4 --coverage",
    "upgrade": "npm upgrade --save"
  },
  "version": "5.1.0"
}
